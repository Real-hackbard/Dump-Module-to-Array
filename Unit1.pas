procedure TForm1.WriteOptions;    // ################### Options Write
var OPT :string;
begin
   OPT := 'Options';

   if not DirectoryExists(ExtractFilePath(Application.ExeName) + 'Options\')
   then ForceDirectories(ExtractFilePath(Application.ExeName) + 'Options\');

   TIF := TIniFile.Create(ExtractFilePath(Application.ExeName) + 'Options\Options.ini');
   with TIF do
   begin
   WriteInteger(OPT,'Code',Combobox1.ItemIndex);
   WriteBool(OPT,'Api',CheckBox1.Checked);
   WriteBool(OPT,'Pause',CheckBox2.Checked);
   WriteBool(OPT,'Vol',CheckBox3.Checked);
   WriteBool(OPT,'Jump',CheckBox4.Checked);
   WriteBool(OPT,'Load',CheckBox5.Checked);
   WriteBool(OPT,'Loop',CheckBox6.Checked);
   WriteBool(OPT,'Mark',CheckBox7.Checked);
   WriteInteger(OPT,'Volume',ScrollBar1.Position);
   Free;
   end;
end;

procedure TForm1.ReadOptions;    // ################### Options Read
var OPT:string;
begin
  OPT := 'Options';
  if FileExists(ExtractFilePath(Application.ExeName) + 'Options\Options.ini') then
  begin
  TIF:=TIniFile.Create(ExtractFilePath(Application.ExeName) + 'Options\Options.ini');
  with TIF do
  begin
  ComboBox1.ItemIndex:=ReadInteger(OPT,'Code',ComboBox1.ItemIndex);
  CheckBox1.Checked:=ReadBool(OPT,'Api',CheckBox1.Checked);
  CheckBox2.Checked:=ReadBool(OPT,'Pause',CheckBox2.Checked);
  CheckBox3.Checked:=ReadBool(OPT,'Vol',CheckBox3.Checked);
  CheckBox4.Checked:=ReadBool(OPT,'Jump',CheckBox4.Checked);
  CheckBox5.Checked:=ReadBool(OPT,'Load',CheckBox5.Checked);
  CheckBox6.Checked:=ReadBool(OPT,'Loop',CheckBox6.Checked);
  CheckBox7.Checked:=ReadBool(OPT,'Mark',CheckBox7.Checked);
  ScrollBar1.Position:=ReadInteger(OPT,'Volume',ScrollBar1.Position);
  Free;
  end;
  end;
end;

function Get_File_Size4(const S: string): Int64;
var
  FD: TWin32FindData;
  FH: THandle;
begin
  FH := FindFirstFile(PChar(S), FD);
  if FH = INVALID_HANDLE_VALUE then Result := 0
  else
    try
      Result := FD.nFileSizeHigh;
      Result := Result shl 32;
      Result := Result + FD.nFileSizeLow;
    finally
      //CloseHandle(FH);  // This can cause crashes
    end;
end;

procedure TForm1.Refresh;
begin
  case ComboBox1.ItemIndex of
  0 : Module := ' /Dm ';
  1 : Module := ' /Dd ';
  2 : Module := ' /Dc ';
  3 : Module := ' /Dc ';
  4 : Module := ' /Ds ';
  end;

  if CheckBox1.Checked = true then begin
    Info := ' /Di '; end else begin Info := ''; end;

  if CheckBox2.Checked = true then begin
    PRS := ' /Dp '; end else begin PRS := ''; end;

  if CheckBox3.Checked = true then begin
    Vol := ' /Dv '; end else begin Vol := ''; end;

  if CheckBox4.Checked = true then begin
    Jump := ' /Dj '; end else begin Jump := ''; end;

  if CheckBox5.Checked = true then begin
    Load := ' /Df '; end else begin Load := ''; end;

  if CheckBox6.Checked = true then begin
    Loop := ' /Dl '; end else begin Loop := ''; end;

  if CheckBox7.Checked = true then begin
  Mark := ' /M '; end else begin Mark := ''; end;

  dump := '/c ' + Path + Module + Info + PRS + Vol + Jump +
                  Load + Loop + Mark + ExtractFileName(OpenDialog1.FileName);
end;

