procedure TForm1.Button2Click(Sender: TObject);
var ext : string;
begin
  Screen.Cursor := crHourGlass;
  Ext := Edit1.Text;
  try
    ShellExecute(Handle, 'open', 'cmd', PChar(dump), PChar(Dir), SW_HIDE);
  finally
    Sleep(300);
    Screen.Cursor := crDefault;

    case ComboBox1.ItemIndex of
    0 : begin
        change := ChangeFileExt(Ext, '.asm');
        StatusBar1.Panels[3].Text := IntToStr(Get_File_Size4(change)) + ' bytes';
        end;

    1 : begin
